{
	"name": "nft-preview-card-component-main",
	"version": "1.0.0",
	"description": "NFT preview card component coding challenge",
	"main": "index.js",
	"scripts": {
		"live": "npx live-server",
		"sass:watch": "npx sass -w scss/style.scss:css/style.css",
		"groupMedia": "npx group-css-media-queries css/style.css css/style.css",
		"postcss:autoprefixer": "npx postcss css/style.css -u autoprefixer -r",
		"sass:compressed": "npx sass css/style.css:css/style.min.css --style compressed",
		"ttf2woff": "npx ttf2woff fonts/Outfit-Light.ttf fonts/Outfit-Light.woff",
		"webp": "npx webpc -f images/image-equilibrium.jpg",
		"build": "npm run groupMedia && npm run postcss:autoprefixer && npm run sass:compressed"
	},
	"keywords": [],
	"author": "Valeri",
	"license": "ISC",
	"dependencies": {
		"live-server": "^1.2.1"
	},
	"devDependencies": {
		"autoprefixer": "^10.4.2",
		"group-css-media-queries": "^1.4.1",
		"postcss": "^8.4.6",
		"postcss-cli": "^9.1.0",
		"sass": "^1.49.7",
		"ttf2woff": "^3.0.0",
		"webp-converter-cli": "^0.0.5"
	},
	"browserslist": [
		"> 0.01%"
	]
}
